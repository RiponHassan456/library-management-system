<html lang="en">
 <head>
  <meta charset="utf-8"/>
  <meta content="width=device-width, initial-scale=1" name="viewport"/>
  <title>
   Event Calendar - EWU Library
  </title>
  <script src="https://cdn.tailwindcss.com">
  </script>
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" rel="stylesheet"/>
  <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;700&amp;display=swap" rel="stylesheet"/>
  <style>
   body {
      font-family: "Open Sans", sans-serif;
    }
  </style>
 </head>
 <body class="bg-white">
  <!-- Navbar -->
  <nav class="bg-[#0a2f56]">
   <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
    <div class="flex items-center justify-between h-16">
     <div class="text-white font-extrabold text-lg leading-none tracking-wide">
      EWU LIBRARY
     </div>
     <div class="hidden md:flex space-x-8 text-sm font-semibold">
      <a class="text-[#3a8bdc] hover:text-[#3a8bdc] flex items-center" href="#">
       ABOUT
       <i class="fas fa-caret-down ml-1 text-[#3a8bdc]">
       </i>
      </a>
      <a class="text-white hover:text-white" href="#">
       SERVICES
      </a>
      <a class="text-white hover:text-white" href="#">
       RESOURCES
       <i class="fas fa-caret-down ml-1">
       </i>
      </a>
      <a class="text-white hover:text-white" href="#">
       SUBJECT GUIDE
       <i class="fas fa-caret-down ml-1">
       </i>
      </a>
      <a class="text-white hover:text-white" href="#">
       DOWNLOADS
      </a>
      <a class="text-white hover:text-white" href="#">
       CONTACTS
      </a>
     </div>
    </div>
   </div>
  </nav>
  <!-- Subheader -->
  <div class="bg-[#121212] text-white">
   <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-4">
    <div class="flex flex-col sm:flex-row sm:items-center sm:space-x-4">
     <a class="text-[#3a8bdc] text-sm font-semibold mb-2 sm:mb-0" href="#">
      Home
     </a>
     <h1 class="text-xl font-normal">
      Event Calendar
     </h1>
    </div>
    <div class="border-b-4 border-[#3a8bdc] w-24 mt-2">
    </div>
   </div>
  </div>
  <!-- Calendar Controls -->
  <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-6">
   <div class="flex items-center justify-between mb-4">
    <div class="flex items-center space-x-2">
     <button aria-label="Previous" class="bg-[#2c3e50] text-white px-3 py-2 rounded focus:outline-none">
      <i class="fas fa-chevron-left">
      </i>
     </button>
     <button aria-label="Next" class="bg-[#2c3e50] text-white px-3 py-2 rounded focus:outline-none">
      <i class="fas fa-chevron-right">
      </i>
     </button>
     <button class="bg-gray-500 text-white px-3 py-2 rounded text-xs font-semibold focus:outline-none">
      today
     </button>
    </div>
    <div class="text-center text-lg font-normal flex-1">
     May 2025
    </div>
    <div class="flex space-x-1">
     <button class="bg-[#2c3e50] text-white px-3 py-2 rounded text-xs font-semibold focus:outline-none">
      month
     </button>
     <button class="bg-[#2c3e50] text-white px-3 py-2 rounded text-xs font-semibold focus:outline-none">
      week
     </button>
     <button class="bg-[#2c3e50] text-white px-3 py-2 rounded text-xs font-semibold focus:outline-none">
      day
     </button>
     <button class="bg-[#2c3e50] text-white px-3 py-2 rounded text-xs font-semibold focus:outline-none">
      list
     </button>
    </div>
   </div>
   <!-- Calendar Table -->
   <table class="w-full border border-gray-300 text-xs sm:text-sm">
    <thead>
     <tr class="bg-white text-center font-bold text-[10px] sm:text-xs">
      <th class="border border-gray-300 py-1">
       Sun
      </th>
      <th class="border border-gray-300 py-1">
       Mon
      </th>
      <th class="border border-gray-300 py-1">
       Tue
      </th>
      <th class="border border-gray-300 py-1">
       Wed
      </th>
      <th class="border border-gray-300 py-1">
       Thu
      </th>
      <th class="border border-gray-300 py-1">
       Fri
      </th>
      <th class="border border-gray-300 py-1">
       Sat
      </th>
     </tr>
    </thead>
    <tbody class="text-[10px] sm:text-xs">
     <tr>
      <td class="border border-gray-300 h-20 align-top text-right pr-1 text-gray-300">
       27
      </td>
      <td class="border border-gray-300 h-20 align-top text-right pr-1 text-gray-300">
       28
      </td>
      <td class="border border-gray-300 h-20 align-top text-right pr-1 text-gray-300">
       29
      </td>
      <td class="border border-gray-300 h-20 align-top text-right pr-1 text-gray-300">
       30
      </td>
      <td class="border border-gray-300 h-20 align-top text-right pr-1">
       1
       <div class="bg-[#3a8bdc] text-white text-[8px] truncate max-w-full rounded-sm mt-1 px-0.5 leading-tight">
        04:51 am Elsevier's 2025 Lecture Series - Take your research to the next level!
       </div>
      </td>
      <td class="border border-gray-300 h-20 align-top text-right pr-1">
       2
       <div class="bg-[#3a8bdc] text-white text-[8px] truncate max-w-full rounded-sm mt-1 px-0.5 leading-tight">
        04:00 pm Library orientat
       </div>
      </td>
      <td class="border border-gray-300 h-20 align-top text-right pr-1">
       3
      </td>
     </tr>
     <tr>
      <td class="border border-gray-300 h-20 align-top text-right pr-1">
       4
      </td>
      <td class="border border-gray-300 h-20 align-top text-right pr-1">
       5
      </td>
      <td class="border border-gray-300 h-20 align-top text-right pr-1">
       6
      </td>
      <td class="border border-gray-300 h-20 align-top text-right pr-1">
       7
      </td>
      <td class="border border-gray-300 h-20 align-top text-right pr-1">
       8
      </td>
      <td class="border border-gray-300 h-20 align-top text-right pr-1">
       9
      </td>
      <td class="border border-gray-300 h-20 align-top text-right pr-1">
       10
      </td>
     </tr>
    </tbody>
   </table>
  </div>
 </body>
</html>