<html lang="en">
 <head>
  <meta charset="utf-8"/>
  <meta content="width=device-width, initial-scale=1" name="viewport"/>
  <title>
   Access to ebooks - EWU Library
  </title>
  <script src="https://cdn.tailwindcss.com">
  </script>
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" rel="stylesheet"/>
  <link href="https://fonts.googleapis.com/css2?family=Open+Sans&amp;display=swap" rel="stylesheet"/>
  <style>
   body {
      font-family: "Open Sans", sans-serif;
    }
  </style>
 </head>
 <body class="bg-white text-gray-900">
  <!-- Header -->
  <header class="bg-[#0c2f56]">
   <div class="max-w-7xl mx-auto flex items-center justify-between px-4 py-3">
    <div class="flex items-center space-x-3">
     <img alt="BRAC University EWU Library logo with white text and icon on dark blue background" class="h-14 w-auto" height="60" src="https://storage.googleapis.com/a1aa/image/e6c6a6dd-eda1-4fbb-ed68-5d3b92052ff9.jpg" width="60"/>
     <div class="text-white font-extrabold text-lg leading-none tracking-wide">
      EWU
      <br/>
      LIBRARY
     </div>
    </div>
    <nav class="hidden md:flex space-x-6 text-white font-semibold text-sm">
     <a class="hover:underline flex items-center gap-1" href="#">
      ABOUT
      <i class="fas fa-caret-down text-xs">
      </i>
     </a>
     <a class="hover:underline flex items-center gap-1" href="#">
      SERVICES
      <i class="fas fa-caret-down text-xs">
      </i>
     </a>
     <a class="text-[#2a6db0] font-semibold flex items-center gap-1" href="#">
      RESOURCES
      <i class="fas fa-caret-down text-xs">
      </i>
     </a>
     <a class="hover:underline flex items-center gap-1 font-semibold" href="#">
      SUBJECT GUIDE
      <i class="fas fa-caret-down text-xs">
      </i>
     </a>
     <a class="hover:underline font-semibold" href="#">
      DOWNLOADS
     </a>
     <a class="hover:underline font-semibold" href="#">
      CONTACTS
     </a>
    </nav>
   </div>
  </header>
  <!-- Subheader -->
  <section class="bg-[#121212] text-white max-w-7xl mx-auto px-4 py-6">
   <a class="text-[#2a6db0] font-semibold text-sm mb-2 inline-block" href="#">
    Home
   </a>
   <h1 class="text-white text-2xl font-normal">
    Access to ebooks
   </h1>
   <div class="border-b-4 border-[#2a6db0] w-36 mt-2">
   </div>
  </section>
  <!-- Main content -->
  <main class="max-w-7xl mx-auto px-4 py-8 flex flex-col md:flex-row md:space-x-6">
   <!-- Left sidebar -->
   <aside class="md:w-1/4 bg-[#2a6db0] text-white p-6 space-y-8">
    <section>
     <h2 class="font-bold text-lg mb-3">
      Access Details
     </h2>
     <p class="text-sm leading-relaxed mb-3">
      On-Campus: No User ID or Password Required
     </p>
     <p class="text-sm leading-relaxed">
      Off-Campus: Athens ID and Password Required
     </p>
    </section>
    <section>
     <h2 class="font-bold text-lg mb-3">
      Authorized Users
     </h2>
     <p class="text-sm leading-relaxed">
      This service is available to faculty, staff, and students of Brac
          University.
     </p>
    </section>
   </aside>
   <!-- Right content -->
   <section class="md:w-3/4 space-y-6 mt-6 md:mt-0">
    <!-- Cambridge University Press -->
    <article class="bg-[#f7f7f7] p-6 flex flex-col md:flex-row md:items-center md:justify-between space-y-4 md:space-y-0">
     <div class="md:w-2/3">
      <h3 class="font-semibold text-[#0c2f56] mb-3">
       Cambridge University Press
      </h3>
      <div class="flex flex-wrap gap-2 text-xs">
       <button class="border border-gray-300 px-3 py-1 rounded text-gray-800 hover:bg-gray-200" type="button">
        Humanities
       </button>
       <button class="border border-gray-300 px-3 py-1 rounded text-gray-800 hover:bg-gray-200" type="button">
        Social Science
       </button>
       <button class="border border-gray-300 px-3 py-1 rounded text-gray-800 hover:bg-gray-200" type="button">
        Law
       </button>
       <button class="border border-gray-300 px-3 py-1 rounded text-gray-800 hover:bg-gray-200" type="button">
        Literature
       </button>
      </div>
     </div>
     <div class="md:w-1/3 flex flex-col items-center md:items-end space-y-3">
      <img alt="Cambridge University Press logo with red and gold shield and black text on white background" class="w-[140px] h-[60px] object-contain" height="60" src="https://storage.googleapis.com/a1aa/image/c1205f92-3d5c-46dd-5afb-9f1fc7e0baa1.jpg" width="140"/>
      <div class="flex space-x-3">
       <button class="bg-gray-600 text-white text-sm px-4 py-1 rounded hover:bg-gray-700" type="button">
        Access
       </button>
       <button class="bg-gray-600 text-white text-sm px-4 py-1 rounded hover:bg-gray-700" type="button">
        Title List
       </button>
      </div>
     </div>
    </article>
    <!-- SAGE eBooks -->
    <article class="bg-[#f7f7f7] p-6 flex flex-col md:flex-row md:items-center md:justify-between space-y-4 md:space-y-0">
     <div class="md:w-2/3">
      <h3 class="font-semibold text-[#0c2f56] mb-3">
       SAGE eBooks
      </h3>
      <div class="flex flex-wrap gap-2 text-xs">
       <button class="border border-gray-300 px-3 py-1 rounded text-gray-800 hover:bg-gray-200" type="button">
        Business
       </button>
       <button class="border border-gray-300 px-3 py-1 rounded text-gray-800 hover:bg-gray-200" type="button">
        Education
       </button>
       <button class="border border-gray-300 px-3 py-1 rounded text-gray-800 hover:bg-gray-200" type="button">
        Geography
       </button>
       <button class="border border-gray-300 px-3 py-1 rounded text-gray-800 hover:bg-gray-200" type="button">
        Politics
       </button>
      </div>
     </div>
     <div class="md:w-1/3 flex flex-col items-center md:items-end space-y-3">
      <img alt="Sage Knowledge logo with blue stylized S and text on white background" class="w-[140px] h-[60px] object-contain" height="60" src="https://storage.googleapis.com/a1aa/image/95085e2e-465b-4f06-cb63-193a025c64b1.jpg" width="140"/>
     </div>
    </article>
   </section>
  </main>
 </body>
</html>